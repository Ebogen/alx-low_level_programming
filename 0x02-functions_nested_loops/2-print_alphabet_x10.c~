#include "main.h"

/**
 * print _alphabet_x10 - prints 10 times the alphabets, in lowercase;
 *
 * Description: write a function that print 10 times the alpabet,
 * followed by a new line
 */
void print_alphabet_x10(void)
{
	int count = 0;
	char letter;

	while (count++ <= 9)
	{
		for (letter = 'a'; letter <= 'z'; letter++)
			_putchar(letter);
		_putchar('\n');
	}
}
